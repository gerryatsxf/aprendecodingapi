# Production Environment Variables Template
# Copy this to .env and set your actual production values

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-here
JWT_EXPIRES_IN=100000000000000

# Stripe Configuration
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_PAYMENT_SUCCESS_WEBHOOK_SECRET_KEY=whsec_your_production_webhook_secret

# Nylas Configuration
NYLAS_MAIN_ACCOUNT_EMAIL=your-production-email@domain.com
NYLAS_CLIENT_ID=your_production_nylas_client_id
NYLAS_CLIENT_SECRET=your_production_nylas_client_secret
NYLAS_MAIN_ACCOUNT_ACCESS_TOKEN=your_production_nylas_access_token

# Webhook URLs
CREATE_MEETING_URL=https://hook.us2.make.com/your_production_webhook_url

# Vonage Configuration
VONAGE_JWT_365_DAYS=your_production_vonage_jwt

# Note: MongoDB connection is handled automatically by Docker Compose
# MONGO_DB_ATLAS_CONNECTION_STRING is set in docker-compose.yml
